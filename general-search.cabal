source-repository head
  type: git
  location: https://github.com/Rick-T/general-search

library
  exposed-modules:
      Data.Search.BFS.Hashable
      Data.Search.BFS.Int
      Data.Search.BFS.Ord
      Data.Search.Classes.ClosedSet
      Data.Search.Classes.OpenSet
      Data.Search.DFS.Hashable
      Data.Search.DFS.Int
      Data.Search.DFS.Internal
      Data.Search.DFS.Ord
      Data.Search.GraphManip
      Data.Search.GraphManip.Internal
      Data.Search.Simple
      Data.Search.Types
      Data.Search.Types.Accumulator
      Data.Search.Types.Graph
      Data.Search.Types.HeuristicCost
      Data.Search.Types.Search
      Data.Search.Types.Search.Internal
      Data.Search.Types.SearchAlgorithm
      Data.Search.Types.SearchGoal
  other-modules:
      Paths_general_search
  hs-source-dirs:
      src/
  ghc-options: -Wall
  build-depends:
      base >=4.7 && <5
    , containers >=0.6 && <0.7
    , hashable >=1.3 && <1.4
    , mtl >=2.2 && <2.3
    , psqueues >=0.2 && <0.3
    , unordered-containers >=0.2 && <0.3
  default-language: Haskell2010

test-suite general-search-test
  type: exitcode-stdio-1.0
  main-is: Spec.hs
  other-modules:
      Data.Search.ExampleGraph
      Data.Search.GraphManipSpec
      Data.Search.Types.AccumulatorSpec
      Data.Search.Types.GraphSpec
      Data.Search.Types.HeuristicCostSpec
      Data.Search.Types.SearchGoalSpec
      Data.Search.Types.SearchSpec
      Paths_general_search
  hs-source-dirs:
      test
  ghc-options: -threaded -rtsopts -with-rtsopts=-N
  build-tool-depends:
      hspec-discover:hspec-discover
  build-depends:
      QuickCheck >=2.13 && <2.14
    , array >=0.5 && <0.6
    , base >=4.7 && <5
    , containers >=0.6 && <0.7
    , general-search
    , hashable >=1.3 && <1.4
    , hspec >=2.7 && <2.8
    , mtl >=2.2 && <2.3
    , psqueues >=0.2 && <0.3
    , unordered-containers >=0.2 && <0.3
  default-language: Haskell2010

benchmark general-search-bench
  type: exitcode-stdio-1.0
